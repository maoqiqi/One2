    <!-- 项目介绍 -->
    <string name="introduction_title">本案例主要演示安卓常用控件的使用。</string>
    <string name="introduction_1">1. 技术点</string>
    <string name="introduction_2">2. 依赖项目</string>
    <string name="introduction_3">3. 参考项目</string>
